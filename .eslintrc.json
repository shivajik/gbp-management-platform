{
  "extends": ["next/core-web-vitals"],
  "rules": {
    "prettier/prettier": "off",
    "no-console": "off",
    "no-debugger": "off",
    "prefer-const": "off",
    "no-var": "warn",
    "object-shorthand": "off",
    "prefer-template": "off",
    "react/no-unescaped-entities": "off",
    "react-hooks/exhaustive-deps": "off",
    "react-hooks/rules-of-hooks": "error",
    "@next/next/no-img-element": "off"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}
